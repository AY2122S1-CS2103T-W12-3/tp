<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="tabPanel" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <Button fx:id="guests" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleClickGuestTab" style="-fx-border-radius: 5 5 0 0; -fx-border-color: #666666; -fx-border-width: 2 2 0 2;" text="Guests" VBox.vgrow="NEVER">
        <padding>
            <Insets top="2.0" />
        </padding>
      <opaqueInsets>
         <Insets left="10.0" />
      </opaqueInsets>
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
    </Button>
    <Button fx:id="vendors" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleClickVendorTab" style="-fx-border-radius: 5 5 0 0; -fx-border-color: #666666; -fx-border-width: 2 2 0 2;" text="Vendors" VBox.vgrow="NEVER">
        <padding>
            <Insets top="2.0" />
        </padding>
      <opaqueInsets>
         <Insets left="10.0" />
      </opaqueInsets>
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
    </Button>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</HBox>
